{
  "name": "fancy-fruit",
  "version": "1.0.0",
  "description": "Fruit sales manager",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": "12.15.0",
    "npm": "6.13.4"
  },
  "scripts": {
    "client:build": "cross-env NODE_ENV=development webpack",
    "client:release": "cross-env NODE_ENV=production webpack",
    "server:eslint": "eslint \"./server/**/*.ts\"",
    "server:release": "rimraf build/server && yarn server:eslint && yarn server:compile",
    "release": "yarn coverage && yarn client:release && yarn server:release && yarn docs",
    "coverage": "nyc yarn test",
    "docs": "typedoc --options typedoc.json  --tsconfig tsconfig.typedoc.json",
    "test": "mocha test/**/*.{ts,tsx}",
    "watch": "cross-env NODE_ENV=development WATCH=true nodemon server/index.ts",
    "start": "node build/server/index.js",
    "nodemon": "cross-env NODE_ENV=development nodemon server/index.ts",
    "clean": "rimraf build && rimraf node_modules && rimraf .coverage && rimraf .nyc_output"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/corinnaerin/fancy-fruit.git"
  },
  "author": "Corinna Kuhn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/corinnaerin/fancy-fruit/issues"
  },
  "homepage": "https://github.com/corinnaerin/fancy-fruit#readme",
  "dependencies": {
    "@types/fancy-log": "^1.3.1",
    "@types/supertest": "^2.0.8",
    "express": "^4.16.3",
    "fancy-log": "^1.3.2",
    "http-errors": "^1.6.3",
    "moment": "^2.24.0",
    "recharts": "^2.0.0-beta.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/express": "^4.11.1",
    "@types/express-serve-static-core": "^4.11.1",
    "@types/http-errors": "^1.6.3",
    "@types/mime": "^2.0.0",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/recharts": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "autoprefixer": "^9.7.4",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "enzyme": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.0.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.3.2",
    "react-addons-test-utils": "^15.6.2",
